{
  "kind": "singleType",
  "collectionName": "site_details",
  "info": {
    "singularName": "site-detail",
    "pluralName": "site-details",
    "displayName": "Site Details",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "default": "Kutch Mahila Vikas Sangathan",
      "required": true,
      "unique": false
    },
    "tagline": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "favicon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "socials": {
      "displayName": "social",
      "type": "component",
      "repeatable": true,
      "component": "global.social"
    },
    "contact": {
      "displayName": "contact",
      "type": "component",
      "repeatable": false,
      "component": "global.contact"
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
